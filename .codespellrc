[codespell]
skip = .git,.gitlab,cmake,3rdparty,*.svg,*.pdf,LICENSES,AUTHORS.md
quiet-level = 2
# Words to be ignored, put one word per line and sort alphabetically
ignore-words-list =
 claus,
 gaus,
 hist,
 oger,
 millepede,
 nam,
 nd,
 pixelx,
